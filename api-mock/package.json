{
  "name": "ticketeria-api-mock",
  "version": "1.0.0",
  "description": "API Mock para Sistema de Ticketeria",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/server.js",
    "dev": "ts-node src/server.ts",
    "json-server": "json-server --watch db.json --port 3000 --middlewares ./middleware.js",
    "json-server:host": "json-server --watch db.json --port 3000 --host 0.0.0.0 --middlewares ./middleware.js"
  },
  "keywords": [
    "mock",
    "api",
    "json-server",
    "ticketeria",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}